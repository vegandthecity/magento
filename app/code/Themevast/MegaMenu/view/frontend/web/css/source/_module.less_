@depth0_bg:#f0f0f0;
@depth0_border:#f0f0f0;
@depth0_color: #333;
@drop_bg: #FFF;
@ver_border:#dfdfdf;
@mb_border:#121212;
@mb_background:transparent;
@mb_color:inherit;

& when (@media-common = true) {
	.tv-menu{
		.cat-tree a.groupdrop-title {font-weight: normal;}
		p.groupdrop-title,.cat-tree.no-dropdown  a.groupdrop-title{font-weight: bold;}
		.space-s{
			margin-bottom:10px;
		}
		.space-top,.groupdrop-title.space-top{
			margin-top:20px;
		}
		.text-uppercase{
			text-transform:uppercase;
		}

		.line-left{
			padding-left:20px;
			border-left:1px solid #ccc;
			.tv-rtl-layout &{
				padding-left:0px;
				padding-right:20px;
				border-right:1px solid #ccc;
				border-left:none;
			}
		}
		.line-right{
			padding-right:20px;
			border-right:1px solid #ccc;
			.tv-rtl-layout &{
				padding-left:20px;
				padding-right:0px;
				border-left:1px solid #ccc;
				border-right:none;
			}
		}

		hr{
			border-top: 1px solid #ccc;
			margin-bottom: 20px;
			margin-top: 20px;
		}
		.text-center{text-align:center;}
		.group-title-bottom{margin-bottom:20px;}
		.groupmenu-drop li.parent{position:relative;}
		.block.widget{
			.block-title{
				text-align:center;
			}
			.products-grid .product-item{
				width:100%!important;
				margin:0;
				border:none;
				text-align:center;
				.product-item-info{
					border:none;
					box-shadow:none;
					margin:0;
					padding:0;
					.actions-primary{
						display:inline;
					}
					.actions-primary + .actions-secondary{
						display:inline;
						width:auto;
					}
				}
			}
		}
		.contact-form-wrapper{
			margin-right:10%;
			margin-bottom:20px;
			.tv-rtl-layout &{
				margin-left:10%;
				margin-right:0;
			}
			.legend{
				font-size:150%;
				border-bottom:none;
			}
			.fieldset{
				margin-bottom:20px;
			}
			.actions-toolbar .primary{
				margin:0;
				.tv-rtl-layout &{
					float:right;
				}
			}
		}
	}
	.temp > .need-unwrap {
		margin: 0 0;
	}
	.tv-rtl-layout{direction: rtl; text-align:right;}
	.tv-menu{
		i.menu-icon.img-icon {
			display: inline-block; 
			margin-right: 5px;
			img{max-width:18px;}
		}
		.groupmenu{
			li.level0 > a.menu-link {
				.menu-icon{
					margin-right:3px;
					.tv-rtl-layout & {
						margin-right:0px;
						margin-left:3px;
					}
				}
			}
		}
	}
}

@media (min-width: 768px) {
	.tv-navigation{
		box-sizing: border-box;
		margin-left: auto;
		margin-right: auto;
		max-width: 1200px;
		padding-left: 10px;
		padding-right: 10px;
		position: relative;
		display:block;
		.tv-horizontal-menu .groupmenu{margin:0;}
	}
	.nav-sections-item-content > .tv-navigation{display:block;}
	.tv-menu{
		li.item.level0{		
			.tv-google-map{
				display:none;
			}
			&:hover{
				.tv-google-map{
					display:block;
				}
			}
		}
		.groupmenu-drop, .groupmenu-drop-content{display:block;}
		img.groupdrop-banner-pos{position: absolute;}
		ul.groupdrop-link, ul.groupmenu-drop, ul.groupmenu-nondrop{padding: 0;}
		.dropdown-rtl{
			text-align:right;		
			.tv-rtl-layout &{
				text-align:left;	
			}	
			li.item.parent {
				> .menu-link:before{
					float: left;
					content: "\f104";
					.tv-rtl-layout &{
						float:right;
						content: "\f105";						
					}
				}
				.groupmenu-drop{
					left: auto; 
					right: 100%;
					text-align:right;
					.tv-rtl-layout &{
						left: 100%; 
						right: auto;
						text-align:left;
					}
				}
			}
			
		}
		/*menu full width*/
		&.dropdown-fullwidth{
			.groupmenu-drop{
				border:none;
			}
		}
		/*end menu full width*/
		.groupmenu-drop{
			position: absolute;
			background: @drop_bg;
			top: 100%;
			z-index: 99;			
			border: 1px solid #ccc;
			padding:0;
			box-shadow: 0 5px 5px rgba(0,0,0,0.19);
			/*tab menu*/
			.tv-vertical-tabs{
				.tv-tabs{
					.tv-nav-tabs{
						padding-right:0;
						.tv-rtl-layout &{
							padding-right:10px;
							padding-left:0;
						}
						.tv-tab-link{
							display:block;
							padding:10px 5px;
							&:after{
								display: inline-block;
								font: normal normal normal 14px/1 FontAwesome;
								font-size: inherit;
								text-rendering: auto;
								-webkit-font-smoothing: antialiased;
								content:"\f105";
								float:right;
								margin-top: 3px;
								.tv-rtl-layout &{
									float:left;
									content:"\f104";
								}
							}
							&.active{
								background:#ececec;
							}
						}
						.tv-nav-tabs{
							padding-left:0;
							padding-right:0;
						}
					}
					.tv-tab-content{
						&.col-sm-17{
							padding-left:0;
							.tv-rtl-layout &{
								padding-left:10px;
								padding-right:0;
							}
						}
						.tv-tab-pane{
							display:none;
							&.active{display:block;}
						}
					}
				}
			}
			.tv-horizontal-tabs{
				.tv-vertical-tabs .tv-tabs .tv-nav-tabs .tv-tab-link{
					display:block;
					padding:10px 5px;
					text-align:left;
					.tv-rtl-layout &{
						text-align:right;
					}
					
				}
				.tv-tabs{
					.tv-nav-tabs{
						text-align:center;
						display:block;
						margin-bottom:15px;
						.tv-tab-link{
							display:inline-block;
							margin:3px 20px;
							padding-bottom:7px;
							position:relative;
							&:before{
								position:absolute;
								left:0;
								right:0;
								bottom:0;
								height:2px;
								background:#dfdfdf;
								transform: scaleX(0);  
								transition: transform 250ms ease-in-out;
								content:"";
							}
							&.active:before{
								transform: scaleX(1); 
							}							
						}
					}
					.tv-tab-content{
						.tv-tab-pane{
							display:none;
							&.active{display:block;}
						}
					}
				}
			}
			/*end tab menu*/
			.groupmenu-drop{
				top:-1px;
				left: 100%;
				visibility: hidden;
				-webkit-opacity: 0;
				-moz-opacity: 0;
				opacity: 0;
				filter: alpha(opacity=0);
				-webkit-transform: translateY(20px);
				-moz-transform: translateY(20px);
				-o-transform: translateY(20px);
				transform: translateY(20px);
				-webkit-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
				-moz-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
				transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
				.tv-rtl-layout &{
					right: 100%;
					left:auto;
				}
			}
			.cat-tree.groupmenu-drop .groupmenu-drop{top: -7px;} 			
			.item{
				&.parent{
					> .menu-link{
						display:block;
						&:before{
							display: inline-block;
							font: normal normal normal 14px/1 FontAwesome;
							font-size: inherit;
							text-rendering: auto;
							-webkit-font-smoothing: antialiased;
							content:"\f105";
							float:right;
							margin-top: 3px;
							.tv-rtl-layout &{
								float:left;
								content:"\f104";
							}
						}
					}
					&.cat-tree:not(.no-dropdown) > a{
						display:block;
						&:before{
							display: inline-block;
							font: normal normal normal 14px/1 FontAwesome;
							font-size: inherit;
							text-rendering: auto;
							-webkit-font-smoothing: antialiased;
							content:"\f105";
							float:right;
							margin-top: 3px;
						}
					}
				}
			}
		}
		.groupmenu-drop  li.item:hover > .groupmenu-drop{
			-webkit-opacity: 1;
			-moz-opacity: 1;
			opacity: 1;
			filter: alpha(opacity=100);
			-moz-transform: translateY(0);
			-webkit-transform: translateY(0);
			-o-transform: translateY(0);
			transform: translateY(0);
			visibility: inherit;
			
		}		
		.groupmenu-drop > li.item{
			min-width: 230px;
			position:relative;
			padding: 20px;			
			margin: 0px auto;
		}
		.cat-tree li.item{
			padding: 8px 15px;
			min-width: 230px;
			position:relative;		
			margin: 0px auto;
		}
    }
	/*menu horizontal*/
	.tv-horizontal-menu{
		width: 100%;
		li.level0 > .groupmenu-drop{
			display:none;
			.tv-rtl-layout &{
				right:0;
				left:auto;
			}
		}
		&.tv-translate{
			li.level0{
				> .groupmenu-drop{
					top:45px;
					display:block;
					visibility: hidden;
					-webkit-opacity: 0;
					-moz-opacity: 0;
					opacity: 0;
					filter: alpha(opacity=0);
					-webkit-transform: translateY(20px);
					-moz-transform: translateY(20px);
					-o-transform: translateY(20px);
					transform: translateY(20px);
					-webkit-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
					-moz-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
					transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
				}
				&:hover > .groupmenu-drop{
					-webkit-opacity: 1;
					-moz-opacity: 1;
					opacity: 1;
					filter: alpha(opacity=100);
					-moz-transform: translateY(0);
					-webkit-transform: translateY(0);
					-o-transform: translateY(0);
					transform: translateY(0);
					visibility: inherit;
					
				}
			}			
		}
		.groupmenu{
			width: 100%;
			display: inline-block;
			padding: 0;
			background-color: @depth0_bg;
			li.level0 > a.menu-link {
				text-transform: uppercase;
				-webkit-transition: all 500ms ease;
				-moz-transition: all 500ms ease;
				transition: all 500ms ease;
				-webkit-backface-visibility: hidden;
				backface-visibility: hidden;
				display: inline-block;
				vertical-align: top;
				padding: 12px 20px;
				box-sizing: border-box;
				border-width: 0px;
				border-style: solid;
				text-decoration: none;
				color: @depth0_color;
				background-color: @depth0_bg;
				border-color: @depth0_border;
				font: 500 12px/1.35 Poppins, Helvetica Neue, Verdana, Arial, sans-serif;
				cursor: pointer;
				line-height: 22px;
			}
			.groupmenu-drop{				
				font: 500 12px/1.35 Poppins, Helvetica Neue, Verdana, Arial, sans-serif;
			}
			li.item {
				list-style: none;
			}
			li.item.level0{
				margin-top: 0px;
				margin-bottom: 0px;
				position: relative;
				display: inline-block;
				vertical-align: top;
				float: left;	
				.tv-rtl-layout &{
					float:right;
				}
			}
			.groupmenu-nondrop {
				position: relative;
				> li.item{
					margin: 0px 0px;
					padding-left: 0px;
				}
			}
			.no-dropdown{
				> .cat-tree.groupmenu-drop {
					top: auto;
					left: auto;
				}
				> .groupdrop-title{
					margin-bottom:10px;
					display: inline-block;
				}
			}
			ul.groupmenu-drop li.no-dropdown > .cat-tree.groupmenu-drop .groupmenu-drop{
				visibility: hidden;
				height: 0;
				position: absolute;
				opacity: 0;
				background: #fff;
				border: 1px solid #ccc;
				height: auto;
			}
			ul.groupmenu-drop li.no-dropdown > .cat-tree.groupmenu-drop .parent:hover > .groupmenu-drop{
				visibility: visible;
				height: auto;
				opacity: 1;
			}
			ul.groupmenu-drop li.no-dropdown {
				display: block;
				list-style: none;
				margin-top:0; margin-bottom:0;
			}
		}
	}
		
	/*menu vertical*/
	.tv-vertical-menu{
		.groupmenu-drop{
			top:-1px;
			left:100%;
			visibility: hidden;
			-webkit-opacity: 0;
			-moz-opacity: 0;
			opacity: 0;
			filter: alpha(opacity=0);
			-webkit-transform: translateY(20px);
			-moz-transform: translateY(20px);
			-o-transform: translateY(20px);
			transform: translateY(20px);
			-webkit-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
			-moz-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
			transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
			display:none;
			.tv-rtl-layout &{
				right:100%;
				left:auto;
			}
		}
		li.item:hover > .groupmenu-drop{
			-webkit-opacity: 1;
			-moz-opacity: 1;
			opacity: 1;
			filter: alpha(opacity=100);
			-moz-transform: translateY(0);
			-webkit-transform: translateY(0);
			-o-transform: translateY(0);
			transform: translateY(0);
			visibility: inherit;
			display:block;
		}		
