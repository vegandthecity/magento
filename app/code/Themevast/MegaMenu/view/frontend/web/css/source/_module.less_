					font-style: normal;
					font-weight: normal;
					font-size: 18px;
					text-rendering: auto;
					-webkit-font-smoothing: antialiased;
					content:"\f105";
					float:right;
					margin-top: 8px;
					margin-right: 10px;
					.tv-rtl-layout &{
						content:"\f104";
						float:left;
						margin-right: 0px;
						margin-left: 10px;
					}
				}
