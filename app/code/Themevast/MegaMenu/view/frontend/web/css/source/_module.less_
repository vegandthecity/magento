@column_width_l:1140px;
@media (min-width: 768px) {
	.generate-columns(24);
	.generate-columns(@n, @i: 1) when (@i =< @n) {
		.tv-menu .groupmenu-width-@{i} {
			width: (@column_width_s / 24 * @i);
		}
		.tv-menu .groupmenu-width-@{i}.row {
			box-sizing: content-box;
		}
		.tv-menu.dropdown-fullwidth .groupmenu-width-@{i}{
			margin-left: calc(~"50% - "@column_width_s / 2);
			margin-right: calc(~"50% - "@column_width_s / 2);
		}
	  .generate-columns(@n, (@i + 1));
	}
}
