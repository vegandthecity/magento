/*menu mobile*/
@media (max-width: 768px) {
	.tv-menu{
		.row{margin:0 -10px;}	
		.groupdrop-banner{margin-bottom:20px;}
		li.item{
			margin-bottom: 0px;
			&.open > .dropdown-toggle:before,
			.tv-tab-link.active + .dropdown-toggle:before{
				-moz-transform: rotate(90deg);
				-o-transform: rotate(90deg);
				transform: rotate(90deg);
			}			
		}
		.menu-tabs{
			margin:0;
			.tab-item{
				margin:0;
			}
			.tv-tab-pane{
				margin-top:10px;
				min-height:1px!important;
			}
			.tv-link-wrap{
				position:relative;
				padding: 4px 0;
			}
		}
		.groupmenu-drop{
			display:none;
			li.item{
				padding:5px 0;
				&.parent .groupmenu-drop{
					padding-left:20px;
					padding-top:5px;
				}
			}
		}
		.menu-link, a.groupdrop-title{
			width: 100%;
			display: inline-block;
		}
		.item.level-top{
			display: block;
			position:relative;
			margin-bottom:5px;
			> .menu-link{
				padding: 10px;
				background: @mb_background;
				color:@mb_color;
				border-bottom:1px solid;
				.fa{margin:0 5px;}
			}
			> .dropdown-toggle {
				width: 40px;
				height: 40px;
				background: darken(@mb_background,10%);
				color:@mb_color;
				&:before{
					margin-top:7px;
				}
			}
			> .groupmenu-drop{
				margin:0px;
				border: 1px solid @depth0_border;
				padding: 10px;
				box-shadow: none;
				-moz-box-shadow: none;
				-webkit-box-shadow: none;
			}
		}
		.parent{
			position:relative;
		}
		.dropdown-toggle {
			display: block;
			float: right;
			position: absolute;
			top: 0;
			right: 0;
			cursor: pointer;
			.tv-rtl-layout &{
				left:0;
				right:auto;
			}
			&:before{
				display: inline-block;
				font-family: FontAwesome;
				font-style: normal;
				font-weight: normal;
				font-size: 27px;
				line-height:1;
				text-rendering: auto;
				-webkit-font-smoothing: antialiased;
				content:"\f105";
				text-align:center;
				width:100%;
				-moz-transition: all 500ms ease;
				-webkit-transition: all 500ms ease;
				-o-transition: all 500ms ease;
				transition: all 500ms ease;
				-moz-backface-visibility: hidden;
				-webkit-backface-visibility: hidden;
				backface-visibility: hidden;
				.tv-rtl-layout &{
					content:"\f104";
				}
			}
		}
	}
}

/*column menu*/
@column_width_s:710px;
